{
    "name": "bee/client",
    "description": "PHP client for Bee.",
    "keywords": ["bee", "client", "pure", "nosql"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Eugene Leonovich",
            "email": "gen.work@gmail.com"
        }
    ],
    "require": {
        "php": "^5.6|^7.0"
    },
    "require-dev": {
        "ext-msgpack": "@dev",
        "rybakit/msgpack": "@dev"
    },
    "suggest": {
        "ext-msgpack": "For using PeclPacker.",
        "rybakit/msgpack": "For using PurePacker."
    },
    "autoload": {
        "psr-4": {
            "Bee\\Client\\": "src/"
        }
    },
    "autoload-dev" : {
        "psr-4": {
            "Bee\\Client\\Tests\\": "tests/"
        }
    }
}
